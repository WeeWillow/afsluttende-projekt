---
import { Breadcrumbs } from "astro-breadcrumbs";
import "astro-breadcrumbs/breadcrumbs.css";

const customHome = [
  {
    index: 0,
    text: "Hjem",
    href: "/",
  },
];
---

<div class="breadcrumb">
  <Breadcrumbs customizeLinks={customHome} />
</div>

<style>
  .breadcrumb {
    margin-top: -4rem;
    padding: 0 1rem;
  }
</style>
